{
  "expo": {
    "name": "gromceri-client",
    "slug": "gromceri-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./material/picture.jpg",
    "splash": {
      "image": "./material/picture.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",

      "adaptiveIcon": {
        "foregroundImage": "./material/picture.jpg",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./material/picture.jpg"
    }
  }
}
